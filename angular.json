{
  "version": 1,
  "projects": {
    "gitbit": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/gitbit",
      "sourceRoot": "apps/gitbit/src",
      "prefix": "gb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/gitbit",
            "index": "apps/gitbit/src/index.html",
            "main": "apps/gitbit/src/main.ts",
            "polyfills": "apps/gitbit/src/polyfills.ts",
            "tsConfig": "apps/gitbit/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/gitbit/src/favicon.ico",
              "apps/gitbit/src/assets"
            ],
            "styles": [
              "apps/gitbit/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/gitbit/src/environments/environment.ts",
                  "with": "apps/gitbit/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "gitbit:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "gitbit:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gitbit:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/gitbit/tsconfig.app.json",
              "apps/gitbit/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/gitbit/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/gitbit/jest.config.js",
            "tsConfig": "apps/gitbit/tsconfig.spec.json",
            "setupFile": "apps/gitbit/src/test-setup.ts"
          }
        }
      }
    },
    "gitbit-e2e": {
      "root": "apps/gitbit-e2e",
      "sourceRoot": "apps/gitbit-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/gitbit-e2e/cypress.json",
            "tsConfig": "apps/gitbit-e2e/tsconfig.e2e.json",
            "devServerTarget": "gitbit:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "gitbit:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/gitbit-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/gitbit-e2e/**"
            ]
          }
        }
      }
    },
    "electron-electron": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/gitbit",
      "sourceRoot": "apps/gitbit/src",
      "prefix": "gb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/electron-electron",
            "index": "apps/gitbit/src/index.html",
            "main": "apps/gitbit/src/main.ts",
            "polyfills": "apps/gitbit/src/polyfills.ts",
            "tsConfig": "apps/gitbit/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/gitbit/src/favicon.ico",
              "apps/gitbit/src/assets",
              {
                "glob": "**/*",
                "input": "apps/electron-electron/src/",
                "ignore": [
                  "**/*.ts"
                ],
                "output": ""
              }
            ],
            "styles": [
              "apps/gitbit/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/gitbit/src/environments/environment.ts",
                  "with": "apps/gitbit/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "electron-electron:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "electron-electron:build:production"
            }
          }
        }
      }
    },
    "gitbit-tabmenu-feature": {
      "projectType": "library",
      "root": "libs/gitbit/tabmenu/feature",
      "sourceRoot": "libs/gitbit/tabmenu/feature/src",
      "prefix": "gb",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/gitbit/tabmenu/feature/tsconfig.lib.json",
              "libs/gitbit/tabmenu/feature/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/gitbit/tabmenu/feature/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/gitbit/tabmenu/feature/jest.config.js",
            "tsConfig": "libs/gitbit/tabmenu/feature/tsconfig.spec.json",
            "setupFile": "libs/gitbit/tabmenu/feature/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "gitbit-terminal-data-access": {
      "projectType": "library",
      "root": "libs/gitbit/terminal/data-access",
      "sourceRoot": "libs/gitbit/terminal/data-access/src",
      "prefix": "gb",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/gitbit/terminal/data-access/tsconfig.lib.json",
              "libs/gitbit/terminal/data-access/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/gitbit/terminal/data-access/**"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/gitbit/terminal/data-access/jest.config.js",
            "tsConfig": "libs/gitbit/terminal/data-access/tsconfig.spec.json",
            "setupFile": "libs/gitbit/terminal/data-access/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "gitbit-terminal-typings": {
      "projectType": "library",
      "root": "libs/gitbit/terminal/typings",
      "sourceRoot": "libs/gitbit/terminal/typings/src",
      "prefix": "gb",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/gitbit/terminal/typings/tsconfig.lib.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/gitbit/terminal/typings/**"
            ]
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nstudio/xplat"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "gitbit"
}