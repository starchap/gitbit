{
  "name": "gitbit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "new": "nx workspace-schematic gitbit",
    "start": "npm run postinstall.web && ng serve gitbit",
    "build": "npm run postinstall.web && ng build gitbit",
    "test": "npm run postinstall.web && ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "npm run postinstall.web && ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "electron-rebuild install-app-deps",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "build.electron": "npm run prepare.electron && ng build electron-electron --prod --base-href ./",
    "build.electron.local": "npm run build.electron && electron dist/apps/electron-electron",
    "build.electron.linux": "npm run build.electron && cd dist/apps/electron-electron && npx electron-builder build --linux",
    "build.electron.windows": "npm run build.electron && cd dist/apps/electron-electron && npx electron-builder build --windows",
    "build.electron.mac": "npm run build.electron && cd dist/apps/electron-electron && npx electron-builder build --mac",
    "prepare.electron": "npm run postinstall.electron && tsc -p apps/electron-electron/tsconfig.json",
    "serve.electron.target": "nx serve electron-electron",
    "serve.electron": "wait-on http-get://localhost:4200/ && electron apps/electron-electron/src --serve",
    "start.electron": "npm run prepare.electron && npm-run-all -p serve.electron.target serve.electron",
    "start.gitbit": "npm run postinstall.web && nx serve gitbit"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.0",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.0",
    "@angular/compiler": "^8.2.0",
    "@angular/core": "^8.2.0",
    "@angular/forms": "^8.2.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.0",
    "@angular/platform-browser-dynamic": "^8.2.0",
    "@angular/router": "^8.2.0",
    "@gitgraph/js": "^1.3.3",
    "@nstudio/web-angular": "^8.1.1",
    "@types/electron": "^1.6.10",
    "core-js": "^2.5.4",
    "rxjs": "~6.4.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.14",
    "@angular/cli": "8.3.14",
    "@angular/compiler-cli": "^8.2.0",
    "@angular/language-service": "^8.2.0",
    "@nrwl/angular": "~8.8.0",
    "@nrwl/cypress": "8.11.0",
    "@nrwl/jest": "8.11.0",
    "@nrwl/workspace": "8.11.0",
    "@nstudio/angular": "8.1.1",
    "@nstudio/electron": "8.1.1",
    "@nstudio/electron-angular": "8.1.1",
    "@nstudio/xplat": "8.1.1",
    "@types/jest": "24.0.9",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cypress": "3.6.1",
    "dotenv": "6.2.0",
    "electron": "^11.5.0",
    "electron-builder": "^20.44.4",
    "electron-installer-dmg": "~3.0.0",
    "electron-packager": "~14.1.0",
    "electron-rebuild": "~1.8.6",
    "electron-reload": "~1.5.0",
    "electron-store": "~5.1.0",
    "electron-updater": "~4.2.0",
    "eslint": "6.1.0",
    "jest": "24.1.0",
    "jest-preset-angular": "7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.5.3",
    "wait-on": "~3.3.0"
  },
  "xplat": {
    "prefix": "gitbit",
    "framework": "angular"
  }
}
